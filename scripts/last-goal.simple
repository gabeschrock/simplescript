function fizzBuzz(num) {
    let mod3 = num % 3;
    let mod5 = num % 5;

    if (mod3 == 0 && mod5 == 0) {
        return "fizzbuzz";
    }
    if (mod3 == 0) {
        return "fizz";
    }
    if (!mod5) {
        return "buzz";
    }
    return string(num);
}

for (let i = 1; i <= 50; i++) {
    print(fizzBuzz(i))
}
